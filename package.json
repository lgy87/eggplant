    "cross-env": "^7.0.3",
    "electron": "^15.0.0",
    "electron-builder": "^22.11.7",
    "electron-devtools-installer": "^3.2.0",
    "electron-releases": "^3.805.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.18.0",
    "slash": "^3.0.0",
    "spectron": "^13.0.0",
    "typescript": "^4.4.3",
    "vite": "^2.5.10"
  },
