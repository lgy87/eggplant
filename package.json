{
  "name": "vite-electron-react-starter",
  "description": "fork of soulsam480/vite-electron-react-starter for react.",
  "private": true,
  "main": "main/index.cjs.js",
  "scripts": {
    "buildEnvTypes": "node ./bin/buildEnvTypes.js",
    "prebuild": "npm run buildEnvTypes",
